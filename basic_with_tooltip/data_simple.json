{
    "style": [
      {
        "selector": "node",
        "style": {
          "shape": "ellipse",
          "label": "data(label)",
          "text-valign": "center",
          "background-color": "#61bffc",
          "border-color": "#000",
          "color": "#000",
          "font-size": "10px",
          "border-width": 1,
          "width": 60,
          "height": 60
        }
      },
      {
        "selector": "edge",
        "style": {
          "width": 2,
          "line-color": "#ccc",
          "target-arrow-color": "#ccc",
          "target-arrow-shape": "triangle",
          "curve-style": "bezier"
        }
      }
    ],
    "elements": {
      "nodes": [
        { "data": { "id": "parent", "label": "Parent Node" } },
        { "data": { "id": "child", "label": "Child Node", "link": "https://www.google.com" } }
      ],
      "edges": [
        { "data": { "source": "parent", "target": "child" } }
      ]
    }
  }
  